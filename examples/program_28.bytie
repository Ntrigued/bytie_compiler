# simple contains check in array
retrieve print from Standard;

Array<Integer> bag = [3,6,9,12];
Integer target = 9;
Integer found = 0;

Integer idx = 0;
while(idx < 4) {
  if(bag[idx] == target) { found = 1; }
  idx = idx + 1;
}
if(found == 1) { print("found " + target); }
else { print("not found"); }