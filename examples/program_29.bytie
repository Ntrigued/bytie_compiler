# multiplication table as strings
retrieve print from Standard;

print("Multiplication table demonstration (3x3)");
print("Generating multiplication table from 1 to 3...");

Integer a = 1;
while(a <= 3) {
  print("Row " + a + ":");
  Integer b = 1;
  while(b <= 3) {
    print(a + "x" + b + "=" + (a*b));
    b = b + 1;
  }
  print("---");  # Separator between rows
  a = a + 1;
}

print("Multiplication table completed.");